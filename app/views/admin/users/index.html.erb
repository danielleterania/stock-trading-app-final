<h1>All Users</h1>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>User Type</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.user_type %></td>
        <td>
          <%= link_to 'Show', admin_user_path(user) %>
          <%= link_to 'Edit', edit_admin_user_path(user) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
